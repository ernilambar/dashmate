{
	"$schema": "../data/schemas/dashmate.json",
	"columns": [
		{
			"id": "col-1",
			"widgets": [
				{
					"id": "sample-html",
					"settings": {
						"allow_scripts": false
					},
					"collapsed": false
				},
				{
					"id": "sample-tabular",
					"settings": {
						"max_items": 5
					},
					"collapsed": false
				}
			]
		},
		{
			"id": "col-2",
			"widgets": [
				{
					"id": "sample-links",
					"settings": {
						"hide_icon": false,
						"display_style": "list"
					},
					"collapsed": false
				}
			]
		},
		{
			"id": "col-3",
			"widgets": [
				{
					"id": "sample-progress-circles",
					"settings": {
						"circles_number": 4,
						"hide_caption": false
					},
					"collapsed": false
				},
				{
					"id": "sample-bar-chart",
					"settings": {
						"bars_number": 6,
						"hide_labels": false,
						"show_values": true
					},
					"collapsed": false
				}
			]
		}
	]
}
